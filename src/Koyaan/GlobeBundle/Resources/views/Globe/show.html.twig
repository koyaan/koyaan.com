<h1>Globe</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Picture</th>
            <td><img src="/squareglobe/image/{{ entity.token }}.png"/></td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Token</th>
            <td>{{ entity.token }}</td>
        </tr>
        <tr>
            <th>Data</th>
            <td>{{ entity.data }}</td>
        </tr>
        <tr>
            <th>Accesstoken</th>
            <td>{{ entity.accessToken }}</td>
        </tr>
        <tr>
            <th>Public</th>
            <td>{{ entity.public }}</td>
        </tr>
        <tr>
            <th>created_at</th>
            <td>{{ entity.created_at.date }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('squareglobe') }}">
            Back to the list
        </a>
    </li>
</ul>