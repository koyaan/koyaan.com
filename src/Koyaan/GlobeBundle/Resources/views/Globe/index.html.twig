<h1>Globe list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Token</th>
            <th>Data</th>
            <th>Accesstoken</th>
            <th>Public</th>
            <th>Picture</th>
            <th>Created at</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('squareglobe_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.token }}</td>
            <td>{{ entity.data }}</td>
            <td>{{ entity.accessToken }}</td>
            <td>{{ entity.public }}</td>
            <td>{{ entity.picture }}</td>
            <td>{{ entity.created_at.date|date("c")}}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('squareglobe_show', { 'id': entity.id }) }}">show</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

